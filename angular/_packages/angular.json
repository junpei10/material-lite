{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "package": {
      "projectType": "library",
      "root": "projects/package",
      "sourceRoot": "projects/package/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/package/tsconfig.lib.json",
            "project": "projects/package/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/package/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/package/src/test.ts",
            "tsConfig": "projects/package/tsconfig.spec.json",
            "karmaConfig": "projects/package/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/package/tsconfig.lib.json",
              "projects/package/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "angular-material-lite": {
      "projectType": "library",
      "root": "projects/angular-material-lite",
      "sourceRoot": "projects/angular-material-lite/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/angular-material-lite/tsconfig.lib.json",
            "project": "projects/angular-material-lite/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/angular-material-lite/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/angular-material-lite/src/test.ts",
            "tsConfig": "projects/angular-material-lite/tsconfig.spec.json",
            "karmaConfig": "projects/angular-material-lite/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/angular-material-lite/tsconfig.lib.json",
              "projects/angular-material-lite/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }},
  "cli": {
    "analytics": "87b6cae6-999f-4164-86ab-564005da717a"
  },
  "defaultProject": "package"
}